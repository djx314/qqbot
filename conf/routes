# Map static resources from the /public folder to the /assets URL path
GET        /staticAssets/*file        archer.controllers.CommonAssetsController.staticAt(root = "/public", file)

GET     /   assist.controllers.Assets.index

POST     /qqbot_event   assist.controllers.Assets.qqbotEvent

GET     /create   archer.controllers.CommonAssetsController.createTable
GET     /clean   archer.controllers.CommonAssetsController.cleanTable

GET /random archer.controllers.CommonAssetsController.random

GET /niubi_random archer.controllers.CommonAssetsController.niubiRandom(message: String)