# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        assist.controllers.Assets.at(file)
GET        /assets        assist.controllers.Assets.root
GET        /staticAssets/*file        assist.controllers.Assets.staticAt(root = "/public", file)
GET        /encode/*file        assist.controllers.Encoder.encodeFile(file)

POST     /uploadEncodedFile   assist.controllers.Encoder.uploadEncodedFile
GET     /uploadSuccessFile   assist.controllers.Encoder.encodeSuccessPage(refer: String)

GET     /tempfile/*file   assist.controllers.Assets.tempFile(file)

GET     /deleteTempDir/*file   assist.controllers.Assets.deleteTempDir(file)
GET     /deleteRootTempDir   assist.controllers.Assets.deleteRootTempDir